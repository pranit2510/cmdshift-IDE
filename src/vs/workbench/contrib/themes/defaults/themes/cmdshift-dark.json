	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#608B4E"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#CE9178"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#9CDCFE"
			}
		},
		{
			"scope": "function",
			"settings": {
				"foreground": "#DCDCAA"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#DCDCAA"
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#4FC1FF"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#B5CEA8"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#569CD6"
			}
		},
		{
			"scope": "meta.embedded",
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"foreground": "#569CD6",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#608B4E",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.raw",
			"settings": {
				"foreground": "#CE9178"
			}
		},
		{
			"scope": "markup.list",
			"settings": {
				"foreground": "#D4D4D4"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"function": "#DCDCAA",
		"variable": "#9CDCFE",
		"parameter": "#9CDCFE",
		"type": "#4EC9B0",
		"class": "#4EC9B0",
		"enum": "#4EC9B0",
		"interface": "#4EC9B0",
		"struct": "#4EC9B0",
		"typeParameter": "#4EC9B0",
		"property": "#9CDCFE",
		"enumMember": "#4FC1FF",
		"event": "#9CDCFE",
		"method": "#DCDCAA",
		"macro": "#C586C0",
		"keyword": "#569CD6",
		"modifier": "#569CD6",
		"comment": "#608B4E",
		"string": "#CE9178",
		"number": "#B5CEA8",
		"regexp": "#D16969",
		"operator": "#D4D4D4"
	}
}